{
    "name": "christhompsontldr/fluxui-devices",
    "description": "FluxUI front-end for managing devices and sessions using diego-ninja/laravel-devices",
    "keywords": ["laravel", "livewire", "fluxui", "devices", "sessions", "security"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Thompson",
            "email": "chris@christhompsontldr.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "livewire/livewire": "^3.0|^4.0",
        "livewire/volt": "^1.0",
        "diego-ninja/laravel-devices": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ChrisThompsonTLDR\\FluxuiDevices\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisThompsonTLDR\\FluxuiDevices\\FluxuiDevicesServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
